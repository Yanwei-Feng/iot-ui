{
  "name": "iot-ui",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "dev": "docz dev",
    "start": "npm run dev",
    "build:doc": "rimraf doc-site && docz build",
    "preview:doc": "docz serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:types": "tsc -p tsconfig.build.json && cpr lib esm",
    "commit": "git-cz",
    "clean": "rimraf lib esm dist",
    "build": "npm run clean && npm run build:types && gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yanwei-Feng/iot-ui.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yanwei-Feng/iot-ui/issues"
  },
  "homepage": "https://github.com/Yanwei-Feng/iot-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@types/react": "^17.0.3",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.3",
    "@umijs/fabric": "^2.5.7",
    "commitizen": "^4.2.3",
    "cpr": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^2.3.1",
    "gatsby-plugin-less": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "husky": "^6.0.0",
    "less": "^3.7.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prismjs": "^1.23.0",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-simple-code-editor": "^0.11.0",
    "react-tooltip": "^4.2.17",
    "react-use": "^17.2.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.2",
    "through2": "^4.0.2",
    "typescript": "^4.2.3"
  },
  "lint-staged": {
    "components/**/*.ts?(x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "components/**/*.less": [
      "stylelint --syntax less --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "prop-types": "^15.7.2"
  },
  "typings": "lib/index.d.ts",
  "module": "esm/index.js",
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.less"
  ]
}
